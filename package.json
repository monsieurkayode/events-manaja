{
  "name": "eventsmanager",
  "version": "1.0.0",
  "description": "An online events manager application",
  "main": "app.js",
  "scripts": {
    "start": "node ./build-server/app.js",
    "start:dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:server": "nodemon --watch server --exec babel-node -- server/app.js",
    "dev:client": "webpack --config webpack.dev.js --watch",
    "prod:server": "babel -d ./build-server ./server -s",
    "prod:client": "webpack --config webpack.prod.js",
    "test:server": "mocha --timeout 500000 --compilers js:babel-core/register server/tests/*",
    "test:client": "jest -u && codecov",
    "coveralls": "nyc npm run test:server && nyc report --reporter=text-lcov | coveralls",
    "test:coverage": "npm run test:client && npm run coveralls",
    "heroku-postbuild": "npm run prod:client && npm run prod:server"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "compression-webpack-plugin": "^1.1.7",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.9",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.8",
    "html-webpack-plugin": "^2.30.1",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0",
    "webpack-merge": "^4.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.2.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "enzyme": "^3.3.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.15.1",
    "nyc": "^11.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "should": "^11.2.1",
    "url-loader": "^0.6.2",
    "webpack-dev-server": "^2.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larrystone/eventsmanager.git"
  },
  "author": "#dreamweave team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/larrystone/eventsmanager/issues"
  },
  "homepage": "https://github.com/larrystone/eventsmanager#readme"
}
